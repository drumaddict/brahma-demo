%section#main-slider.no-margin
  .carousel.slide
    %ol.carousel-indicators
      -@main_slider_images.each_with_index do |slide,index|
        -klass=index==0? 'active' : ''
        %li{"data-slide-to" => "#{index}", "data-target" => "#main-slider", class: "#{klass}" }
    .carousel-inner
      -@main_slider_images.each_with_index do |slide,index|
        -klass=index==0? 'active' : ''
        .item{:style => "background-image: url('#{slide.becomes(MediaCollectionMedium).medium.url(:slide_1400_730)}')", class: "#{klass}" }
          .container
            .row.slide-margin
              .col-sm-6
                .carousel-content
                  %h1.animation.animated-item-1=slide.title
                  %h2.animation.animated-item-2=slide.description
                  =link_to 'Read More', slide.link, class: 'btn-slide animation animated-item-3'
              .col-sm-6.hidden-xs.animation.animated-item-4
  %a.prev.hidden-xs{"data-slide" => "prev", :href => "#main-slider"}
    %i.fa.fa-chevron-left
  %a.next.hidden-xs{"data-slide" => "next", :href => "#main-slider"}
    %i.fa.fa-chevron-right


